# Moveware API Configuration
# Copy this file to .env.local and fill in your actual values

# Moveware API Base URL
NEXT_PUBLIC_MOVEWARE_API_URL=https://api.moveware.com/v1

# Moveware API Key (keep this secret - only use in server-side code)
MOVEWARE_API_KEY=your_api_key_here

# Moveware API Secret (keep this secret - only use in server-side code)
MOVEWARE_API_SECRET=your_api_secret_here

# Application Environment
NODE_ENV=development

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional: Enable debug logging
DEBUG=false